	<script type="text/javascript">
			function setExp() 
			{
				var dateString="2020-10-15";
				var today = new Date();
				var joinDate = new Date(dateString);
				var yxp = today.getFullYear() - joinDate.getFullYear();
				var mxp = today.getMonth() - joinDate.getMonth();
				if (mxp < 0) 
				{
					yxp--;
					mxp=12+mxp;
				}
				document.getElementById("eYear").innerHTML = yxp;
				document.getElementById("eMonth").innerHTML = mxp;
				document.getElementById("pYear").innerHTML = yxp+2;
			}
			window.onload = setExp;
        </script>
